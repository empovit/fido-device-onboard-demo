---
session_store_driver:
  Directory:
    path: {{ component_directory }}/manufacturing_sessions/
ownership_voucher_store_driver:
  Directory:
    path: {{ component_directory }}/owner_vouchers/
public_key_store_driver:
  Directory:
    path: {{ component_directory }}/manufacturer_keys/
bind: 0.0.0.0:8080
rendezvous_info:
- ip: {{ rendezvous_host }}
  device_port: 8082
  owner_port: 8082
  protocol: http
protocols:
  plain_di: false
  diun:
    key_path: {{ component_directory }}/keys/diun_key.der
    cert_path: {{ component_directory }}/keys/diun_cert.pem
    key_type: SECP256R1
    mfg_string_type: SerialNumber
    allowed_key_storage_types:
    - FileSystem
    - Tpm
manufacturing:
  manufacturer_cert_path: {{ component_directory }}/keys/manufacturer_cert.pem
  manufacturer_private_key: {{ component_directory }}/keys/manufacturer_key.der
  owner_cert_path: {{ component_directory }}/keys/owner_cert.pem
  device_cert_ca_private_key: {{ component_directory }}/keys/device_ca_key.der
  device_cert_ca_chain: {{ component_directory }}/keys/device_ca_cert.pem
